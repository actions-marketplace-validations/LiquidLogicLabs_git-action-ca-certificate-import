{
  "name": "ca-certificate-import-action",
  "version": "1.1.1",
  "description": "GitHub Action for installing custom SSL/TLS certificates",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/LiquidLogicLabs/git-action-ca-certificate-import.git"
  },
  "scripts": {
    "release:patch": "standard-version --release-as patch --skip.changelog && git push --follow-tags",
    "release:minor": "standard-version --release-as minor --skip.changelog && git push --follow-tags",
    "release:major": "standard-version --release-as major --skip.changelog && git push --follow-tags",
    "build": "echo 'Build completed - action is ready for packaging'",
    "test": "act -j test",
    "test:local": "act -j test --local",
    "ci:local": "act push --local",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "devDependencies": {
    "standard-version": "^9.5.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
